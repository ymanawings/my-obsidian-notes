	<gx:Tour>
    <name>FlightMovie</name>

    <!--Move camera to start point and wait 1sec-->
    <gx:Playlist><gx:AnimatedUpdate>
          <gx:duration>1</gx:duration>
          <Update>
            <targetHref></targetHref>
            <Change>
                <IconStyle targetId="PAPI_SecondLeft">
                  <color>FF0000FF</color>
                </IconStyle>
            </Change>
          </Update>
          <gx:delayedStart>64.0641806361232</gx:delayedStart>   
        </gx:AnimatedUpdate><gx:AnimatedUpdate>
          <gx:duration>1</gx:duration>
          <Update>
            <targetHref></targetHref>
            <Change>
                <IconStyle targetId="PAPI_FarLeft">
                  <color>FF0000FF</color>
                </IconStyle>
            </Change>
          </Update>
          <gx:delayedStart>76.11396385491</gx:delayedStart>   
        </gx:AnimatedUpdate><gx:FlyTo>
            <name>StartPoint</name>
            <Camera>
                <longitude>125.254861966826</longitude>
                <latitude>24.7364999239967</latitude>
                <altitude>344.813387431552</altitude>
                <heading>38.31</heading>
                <tilt>90</tilt>
                <roll>0</roll>
                <gx:altitudeMode>absolute</gx:altitudeMode>
            </Camera>
        </gx:FlyTo>
        <gx:Wait><gx:duration>1</gx:duration></gx:Wait>
        
        <!-- Movie start  -->
        <gx:FlyTo>    
            <gx:flyToMode>smooth</gx:flyToMode>
            <Camera>
                <longitude>125.254861966826</longitude>
                <latitude>24.7364999239967</latitude>
                <altitude>344.813387431552</altitude>
                <heading>38.31</heading>
                <tilt>90</tilt>
                <roll>0</roll>
                <gx:altitudeMode>absolute</gx:altitudeMode>
            </Camera>
        </gx:FlyTo>

     
        <!-- Stop camera at touchdown point-->
        <gx:FlyTo>
            <name>TouchdownPoint</name>
            <gx:duration>81.2875939849624</gx:duration>
            <gx:flyToMode>smooth</gx:flyToMode>
            <Camera>
                <longitude>125.290835378128</longitude>
                <latitude>24.777842656416</latitude>
                <altitude>37.7952</altitude>
                <heading>38.31</heading>
                <tilt>90</tilt>
                <roll>0</roll>
                <gx:altitudeMode>absolute</gx:altitudeMode>
